{
  "expo": {
    "name": "INRN Project",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo/IconApp.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./src/assets/logo/splashApp.png",
      
      "backgroundColor": "#101010"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo/IconApp.png",
        "backgroundColor": "#101010"
      },
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.desplayshido.INRN.Project"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "30.0.2",
            "kotlinVersion": "1.7.10"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "fc535a33-4741-434e-98e8-f2b81710feb6"
      }
    }
  }
}
